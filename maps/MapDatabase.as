package maps{
	public class MapDatabase {
		public static function getMapData(map:int) {
			var mapCode;
			var mapName;
			switch (map) {
				case 0 :
					mapCode="";
					mapName="";
					break;
				//PERKINS HALL				
				
				case 1 ://Clarissa's Room
					mapCode="15:20:(9,12)";
					mapName="Clarissa's Room";
					break;
				case 2 ://Perkins Hall 2F
					mapCode="31:71:(66,20)";
					mapName="Perkins Hall 2F"
					break;
					
					
				default :
					mapCode="";
					mapName="";
					break;
			}
			return new Array(mapCode, mapName);
		}
	}
}